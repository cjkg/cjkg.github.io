$url = "http://www.cnn.com/"
$IE= new-object -ComObject "InternetExplorer.Application"
$IE.navigate2($url)

 while ($IE.busy) {
    start-sleep -milliseconds 1000
 }

$IE.visible=$true 
$link = $ie.Document.IHTMLDocument3_getElementsByTagName('A') | where-object {$_.innerText -eq 'Video'}
$link.Click()
